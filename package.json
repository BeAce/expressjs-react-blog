{
  "name": "express-react",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "browser": "cross-env NODE_ENV=development webpack-dev-server --open",
    "api": "cross-env NODE_ENV=development nodemon ./bin/www",
    "webpack": "cross-env NODE_ENV=development webpack --colors --watch",
    "build": "rm -rf build && cross-env NODE_ENV=production webpack --config webpack.prod.config.js -p --progress --color",
    "eslint": "eslint *.js server/**/ src/**/ --ext .js,.jsx",
    "release": "standard-version -- --dry-run",
    "generate": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "rules": {
      "react/no-danger": 0,
      "import/no-unresolved": 2,
      "no-plusplus": 0,
      "react/jsx-filename-extension": 0,
      "linebreak-style": 0,
      "radix": 0,
      "no-class-assign": 0,
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/alt-text": 0,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "jsx-a11y/no-static-element-interactions": 0,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-no-target-blank": 0,
      "react/require-default-props": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "require-yield": 0,
      "import/no-webpack-loader-syntax": 0
    }
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "bootstrap": "4.0.0-beta",
    "co": "^4.6.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "express": "~4.13.4",
    "highlight.js": "^9.9.0",
    "lodash": "^4.17.2",
    "markdown": "^0.5.0",
    "marked": "^0.3.6",
    "mongoose": "^4.7.2",
    "morgan": "~1.7.0",
    "popper.js": "^1.12.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "^3.0.0",
    "redux-saga": "^0.15.6",
    "serve-favicon": "~2.3.0",
    "webpack": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "colors": "^1.1.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-redux-saga": "^0.4.0",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "nodemon": "^1.12.0",
    "offline-plugin": "^4.8.4",
    "standard-version": "^4.2.0",
    "style-loader": "^0.19.0",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
