{
  "name": "express-react",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node setup",
    "browser": "cross-env NODE_ENV=development webpack-dev-server --open",
    "api": "cross-env NODE_ENV=development nodemon ./bin/www",
    "webpack": "cross-env NODE_ENV=development webpack --colors --watch",
    "build": "rm -rf build && cross-env NODE_ENV=production webpack --config webpack.prod.config.js -p --progress --color",
    "eslint": "eslint *.js server/**/ src/**/ --ext .js,.jsx",
    "release": "standard-version -- --dry-run",
    "generate": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "bootstrap": "4.0.0-beta",
    "co": "^4.6.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "exports-loader": "^0.6.4",
    "express": "~4.13.4",
    "highlight.js": "^9.9.0",
    "lodash": "^4.17.2",
    "markdown": "^0.5.0",
    "marked": "^0.3.6",
    "mongoose": "^4.7.2",
    "morgan": "~1.7.0",
    "popper.js": "^1.12.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-router-scroll": "^0.4.3",
    "redux-saga": "^0.15.6",
    "serve-favicon": "~2.3.0",
    "webpack": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "colors": "^1.1.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-redux-saga": "^0.4.0",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.0",
    "offline-plugin": "^4.8.4",
    "postcss-loader": "^2.0.8",
